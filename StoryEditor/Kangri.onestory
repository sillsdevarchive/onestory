			</ConsultantConversation>
			<ConsultantConversation round="1">
			  <ConsultantNote Direction="ConsultantToCrafter" guid="31cedbcb-5c7e-4e9e-a810-e1192964afb1">BE1: "One day" or "One night"?</ConsultantNote>
			  <ConsultantNote Direction="CrafterToConsultant" guid="da44cd2c-4d66-4dca-a884-395a532fc721">KD2: One night</ConsultantNote>
			</ConsultantConversation>
